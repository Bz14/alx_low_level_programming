#include<stdio.h>
/**
 * main - Entry Point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
int i = 0, j, k;
while (i < 8)
{
j = i + 1;
while (j <= 9)
{
k = j + 1;
while ( k <= 9)
{
putchar(48 + i);
putchar(48 + j);
putchar(48 + k);
if (i != 7)
{
putchar(',');
putchar(' ');
}
k++;
}
j++;
}
i++;
}
putchar('\n');
return (0);
}
